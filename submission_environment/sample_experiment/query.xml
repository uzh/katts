<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<query xmlns="http://uzh.ch/ddis/katts/query" defaultBufferTimeout="-1">
    <fileSource id="6071725272690409238">
        <files>
            <file csvFieldDelimiter="," mimeType="text/comma-separated-values" isZipped="true" zipFileEntry="triples-sample.csv" path="$JOB_TMP_DIR/data/triples-sample.csv.zip"/>
        </files>
    </fileSource>
    <tripleFilter groupOn="subject" applyOnSource="6071725272690409238" id="5391089872447277452">
        <conditions>
